% Note in tables and figures

\newcommand{\notesize}{\fontsize{5}{5}\selectfont}

\newboolean{iscaptionnote}
\setboolean{iscaptionnote}{false}
\newcommand{\@captionnote}{}
\newcommand{\captionnote}[1]{\renewcommand{\@captionnote}{#1}\setboolean{iscaptionnote}{true}}

\newboolean{isafternote}
\setboolean{isafternote}{false}

\newcommand{\@afternote}{}
\newcommand{\afternote}[2][]{%
    \global\isafternotetrue
    \ifthenelse{\equal{#1}{}}{
        \protected@xdef\@afternote{\@afternote\protect\item #2}
    }{
        \protected@xdef\@afternote{\@afternote\protect\item[\ensuremath{^{#1}}] #2}
    }
}

\newcounter{tnc}

\newcommand{\@tablenote}{}
\newcommand{\tablenote}[1]{%
    \stepcounter{tnc}%
    \hbox{\notesize\ensuremath{\,{}^{\alph{tnc})}}}
    \global\isafternotetrue
    \protected@xdef\@tablenote{\@tablenote\protect\item[\ensuremath{^{\alph{tnc})}\,}] #1}
}

\newcommand{\tablenotesign}[1]{%
    \hbox{\notesize\ensuremath{\,{}^{#1)}}}
}

\newboolean{issig}
\setboolean{issig}{false}
\newboolean{issigg}
\setboolean{issigg}{false}
\newboolean{issiggg}
\setboolean{issiggg}{false}

\newcommand{\sig}{\global\issigtrue\global\isafternotetrue\hbox{\notesize\ensuremath{{}^*}}}
\newcommand{\sigg}{\global\issiggtrue\global\isafternotetrue\hbox{\notesize\ensuremath{{}^{*\!\!*}}}}
\newcommand{\siggg}{\global\issigggtrue\global\isafternotetrue\hbox{\notesize\ensuremath{{}^{*\!\!*\!\!*}}}}

\newcommand{\@signote}{
    \ifthenelse{\boolean{issig}}{
        \ifthenelse{\boolean{issiggg}}{
            \item[\ensuremath{\!{}^{\phantom{*\!\!*\!\!}*}}] Significant at the 10\% level.
        }{
            \ifthenelse{\boolean{issigg}}{
                \item[\ensuremath{\!{}^{\phantom{*\!\!}*}}] Significant at the 10\% level.
            }{
                \item[\ensuremath{\!{}^{*}}] Significant at the 10\% level.
            }
        }
    }{}
    \ifthenelse{\boolean{issigg}}{
        \ifthenelse{\boolean{issiggg}}{
            \item[\ensuremath{\!{}^{\phantom{*\!\!}*\!\!*}}] Significant at the 5\% level.
        }{
            \item[\ensuremath{\!{}^{*\!\!*}}] Significant at the 5\% level.
        }
    }{}
    \ifthenelse{\boolean{issiggg}}{
        \item[\ensuremath{\!{}^{*\!\!*\!\!*}}] Significant at the 1\% level.
    }{}
}

